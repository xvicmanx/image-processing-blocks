{
  "extends": "airbnb",
  "rules": {
    "strict": [
      0,
      "global"
    ],
    "func-names": 0,
    "object-shorthand": 0,
    "consistent-return": 0,
    "prefer-template": 0,
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ],
    "linebreak-style": 0,
    "indent": [
      "error",
      2
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
    "arrow-body-style": [
      "error",
      "always"
    ]
  },
  "plugins": [
    "jsx-a11y"
  ],
  "env": { "browser": true }
}
